    "definitions": {
        "model.Status": {
            "type": "string",
            "enum": [
                "not_started",
                "in_progress",
                "completed"
            ],
            "x-enum-varnames": [
                "NotStarted",
                "InProgress",
                "Completed"
            ]
        },
        "model.Todo": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "due_date": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "priority": {
                    "type": "integer"
                },
